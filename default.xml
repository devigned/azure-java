<?xml version="1.0" encoding="UTF-8"?>
  <manifest>
    <remote name="azure" fetch="git://github.com/azure/"/>
    <remote name="devigned" fetch="git://github.com/devigned/"/>
    <default revision="master" remote="azure" />

    <!-- Java projects -->
    <project name="azure-sdk-for-java" path="java/azure-sdk" groups="java" />
    <project name="azure-storage-java" path="java/storage" groups="java" />
    <project name="azure-documentdb-java" path="java/documentdb" groups="java" />
    <project name="azure-notificationhubs-java-backend" path="java/notificationhubs" groups="java" />

    <!-- Ruby projects -->
    <project name="azure-sdk-for-ruby" path="ruby/azure-sdk" groups="ruby" />

    <!-- Nodejs projects -->
    <project name="azure-sdk-for-node" path="node/azure-sdk" groups="node" />
    <project name="azure-mobile-apps-node" path="node/mobile-apps" groups="node" />
    <project name="azure-storage-node" path="node/storage" groups="node" />
    <project name="azure-documentdb-node" path="node/documentdb" groups="node" />
    <project name="node-sqlserver" path="node/sqlserver" groups="node" />

    <!-- CLI projects -->
    <project name="azure-xplat-cli" path="cli/azure-cli" groups="cli" />

    <!-- Docs generation project 
    <project name="azure-docs" remote="devigned" path="azure-docs" groups="docs" />
    -->
  </manifest>
